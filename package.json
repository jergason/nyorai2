{
  "name": "nyorai2",
  "version": "1.0.0",
  "description": "A CLI tool for indexing directories with Pinecone and OpenAI",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "setup": "npm install && npm run build",
    "link": "npm run build && chmod +x dist/index.js && ln -sf $(pwd)/dist/index.js /usr/local/bin/nyorai2",
    "clean": "rm -rf dist"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.4.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^3.0.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "openai": "^4.56.0",
    "path": "^0.12.7"
  }
}
